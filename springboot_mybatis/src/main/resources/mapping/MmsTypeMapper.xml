<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.it.dao.MmsTypeMapper" >
    <resultMap id="ResultMap" type="com.it.domain.MmsType" >
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="name" property="name" jdbcType="VARCHAR" />
        <result column="status" property="status" jdbcType="VARCHAR" />
    </resultMap>
    <sql id="Column_List" >
        id, name, status
    </sql>
  <!--  <select id="getMmsTypeList" resultMap="ResultMap" parameterType="java.lang.Integer" >
        select
        <include refid="Column_List" />
        from mms_type
    </select>-->


    <!-- 查询 -->
    <select id="getMmsTypeList" parameterType="String" resultMap="ResultMap">
        SELECT * FROM mms_type WHERE id = #{id}
    </select>

</mapper>